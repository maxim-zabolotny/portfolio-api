    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.2",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@types/sharp": "^0.28.5",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "bluebird-retry": "^0.11.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "dotenv": "^8.2.0",
    "googleapis": "^71.0.0",
    "multer": "^1.4.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
