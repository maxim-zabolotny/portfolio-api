  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:local": "NODE_ENV=local nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=3500 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:start": "pm2 start",
    "pm2:stop": "pm2 stop back",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm -c default migration:generate",
    "migration:run": "typeorm -c default migration:run",
    "migration:revert": "typeorm -c default migration:revert",
    "seed:create": "typeorm -c seeds migration:create",
    "seed:run": "typeorm -c seeds migration:run",
    "seed:revert": "typeorm -c seeds migration:revert"
  },
  "dependencies": {
